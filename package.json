{
  "name": "lunch-admin-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.8.1",
    "@material-ui/icons": "^3.0.1",
    "auth0-js": "9.8.2",
    "eslint-plugin-react-hooks": "^1.6.0",
    "history": "4.7.2",
    "http-proxy-middleware": "^0.19.1",
    "material-ui-chip-input": "1.1.0",
    "notistack": "0.8.2",
    "papaparse": "5.2.0",
    "react": "16.8.0",
    "react-bootstrap": "1.0.0-beta.8",
    "react-dom": "16.8.0",
    "react-redux": "7.0.2",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "react-select": "3.0.8",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "styled-components": "^3.2.6",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start cypress": "REACT_APP_MOCK=true react-scripts start",
    "start-now": "serve --single ./build",
    "build": "react-scripts build",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "now -e NODE_ENV=production --token $NOW_TOKEN --public",
    "alias": "now alias --token=$NOW_TOKEN",
    "start:local": "docker-compose -f docker-compose_dev.yml -p lunch_app up -d --build",
    "test:unit": "docker-compose -f docker-compose_test.yml build test && docker-compose -f docker-compose_test.yml run --rm test",
    "build:run:cypress": "cd e2e/ && docker-compose build && docker-compose up --abort-on-container-exit",
    "build:latest": "docker build -t m4rc0z/lunch-admin-app:latest -f Dockerfile .",
    "publish:latest": "docker push m4rc0z/lunch-admin-app:latest",
    "build:dev": "docker build -t m4rc0z/lunch-admin-app:dev.mealit.de -f Dockerfile .",
    "publish:dev": "docker push m4rc0z/lunch-admin-app:dev.mealit.de",
    "build:prod": "docker build --build-arg auth_domain=$AUTH_DOMAIN --build-arg auth_client_id=$AUTH_CLIENT_ID --build-arg auth_audience=$AUTH_AUDIENCE --build-arg auth_container=$AUTH_CONTAINER --build-arg callback_url=$CALLBACK_URL --build-arg backend_url=$BACKEND_URL -t m4rc0z/lunch-admin-app:admin.mealit.de -f Dockerfile .",
    "publish:prod": "docker push m4rc0z/lunch-admin-app:admin.mealit.de",
    "docker:login": "echo $DOCKER_PWD | docker login -u $DOCKER_LOGIN --password-stdin"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "0.72.0",
    "json-server": "0.14.2",
    "mocha": "5.2.0",
    "prop-types": "15.6.2",
    "serve": "6.5.7"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
